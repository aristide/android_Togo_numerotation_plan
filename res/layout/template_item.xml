<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:layout_height="wrap_content"
	android:orientation="vertical">
	<TextView android:id="@+id/nom_contacts" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:paddingLeft="10px"
		android:textColor="@color/vert1" android:textStyle="bold"></TextView>
	<TextView android:id="@+id/ancien_contacts"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:paddingLeft="10px" android:textColor="@color/rouge"
		android:textStyle="bold"></TextView>
	<TextView android:id="@+id/nouveau_contacts"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:paddingLeft="10px" android:textColor="@color/vert1"
		android:textStyle="bold"></TextView>
</LinearLayout>
